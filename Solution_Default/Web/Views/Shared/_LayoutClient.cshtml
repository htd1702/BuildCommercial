<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
    <link href="~/Content/client/material-kit.css" rel="stylesheet" />
    <link href="~/Content/client/Main.css" rel="stylesheet" />
</head>
<body>
    <div>
        @RenderBody()
    </div>
    <!--Footer-->
    <footer class="footer">
        <div class="container">
            <div class="copyright text-center">
                &copy;
                <script>
                    document.write(new Date().getFullYear())
                </script>
                <!--<i class="material-icons">favorite</i> by-->
                <span>Footer</span>
            </div>
        </div>
    </footer>
    <!--End Footer-->
</body>
</html>
<script src="~/Scripts/plugins/jquery/jquery.min.js"></script>
<script src="~/Scripts/plugins/popper/popper.min.js"></script>
<script src="~/Scripts/plugins/bootstrap/bootstrap-material-design.min.js"></script>
<script src="~/Scripts/plugins/jquery/moment.min.js"></script>
<script src="~/Scripts/plugins/bootstrap/bootstrap-datetimepicker.js"></script>
<script src="~/Scripts/plugins/jquery/nouislider.min.js"></script>
<script src="~/Scripts/plugins/jquery/jquery.sharrre.js"></script>
<script src="~/Scripts/client/material-kit.min.js"></script>
<script src="~/Scripts/client/Main.js"></script>